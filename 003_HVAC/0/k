FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

kInlet 0.0045375; // approx k = 1.5*(I*U)^2, I = 0.05, U = 1.1

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $kInlet;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform $kInlet;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform $kInlet;
        value           uniform $kInlet;
    }

    "(top|bottom|walls)"
    {
        type            kqRWallFunction;
        value           uniform $kInlet;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
