convertToMeters 0.1;

vertices
(
    (0 0 0)
    (15 0 0)
    (15 10 0)
    (0 10 0)
    (0 0 5)
    (15 0 5)
    (15 10 5)
    (0 10 5)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (30 20 10) simpleGrading (((0.5 0.5 10)(0.5 0.5 0.1)) 1 1)
);

boundary
(
    movingWall
    {
        type wall;
        faces
        (
            (3 7 6 2)
        );
    }
    fixedWalls
    {
        type wall;
        faces
        (
            (0 4 7 3)
            (2 6 5 1)
            (1 5 4 0)
        );
    }
    frontAndBack
    {
        type empty;
        faces
        (
            (0 3 2 1)
            (4 5 6 7)
        );
    }
);


